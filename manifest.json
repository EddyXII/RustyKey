{
  "manifest_version": 2,
  "name": "RustyKey",
  "version": "1.0",

  "description": "A Browser Extension for rsPass compatible with Firefox",

  "icons": {
  "48": "icons/border-48.png",
  "96": "icons/border-96.png"
  },


  "browser_specific_settings": {
  "gecko": {
    "id": "RustyKey@rsPass"
  }
  },


  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["borderify.js"]
    }
  ]
}
